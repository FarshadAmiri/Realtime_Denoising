			"name": "1. File Denoise Service",
			"item": [
				{
					"name": "Upload Audio File for Denoising",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "X-CSRFToken",
								"value": "{{csrf_token}}",
								"type": "text"
							}
						],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "audio_file",
									"type": "file",
									"src": []
								},
								{
									"key": "boost_level",
									"value": "2x",
									"type": "text",
									"description": "Options: none, 2x, 3x, 4x, 5x"
								}
							]
						},
						"url": {
							"raw": "{{base_url}}/api/denoise/upload/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"denoise",
								"upload",
								""
							]
						},
						"description": "Upload an audio file for noise reduction. Optionally apply volume boost."
					},
					"response": []
				},
				{
					"name": "List Denoised Files",
